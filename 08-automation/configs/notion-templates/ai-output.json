{
  "template_name": "AI Agent Output",
  "database_schema": {
    "properties": {
      "Agent Type": {
        "type": "select",
        "select": {
          "options": [
            {"name": "Code Architect", "color": "blue"},
            {"name": "Research Analyst", "color": "green"}, 
            {"name": "Content Strategist", "color": "purple"},
            {"name": "Business Analyst", "color": "orange"},
            {"name": "Product Manager", "color": "red"}
          ]
        }
      },
      "Created": {
        "type": "date",
        "date": {}
      },
      "Status": {
        "type": "select",
        "select": {
          "options": [
            {"name": "Generated", "color": "yellow"},
            {"name": "Under Review", "color": "blue"},
            {"name": "Approved", "color": "green"},
            {"name": "Needs Revision", "color": "red"},
            {"name": "Implemented", "color": "purple"}
          ]
        }
      },
      "Quality Score": {
        "type": "number",
        "number": {"format": "number"}
      },
      "Review Required": {
        "type": "checkbox",
        "checkbox": {}
      },
      "Implementation Time": {
        "type": "number", 
        "number": {"format": "number"}
      },
      "Tags": {
        "type": "multi_select",
        "multi_select": {
          "options": [
            {"name": "Frontend", "color": "blue"},
            {"name": "Backend", "color": "green"},
            {"name": "Database", "color": "red"},
            {"name": "Research", "color": "purple"},
            {"name": "Content", "color": "orange"},
            {"name": "Strategy", "color": "pink"}
          ]
        }
      }
    }
  },
  "page_template": {
    "children": [
      {
        "object": "block",
        "type": "heading_1",
        "heading_1": {
          "rich_text": [{"text": {"content": "AI Agent Output"}}]
        }
      },
      {
        "object": "block",
        "type": "callout",
        "callout": {
          "rich_text": [{"text": {"content": "⚡ Generated by AI Agent - Requires human review"}}],
          "icon": {"emoji": "⚡"},
          "color": "yellow_background"
        }
      },
      {
        "object": "block",
        "type": "heading_2",
        "heading_2": {
          "rich_text": [{"text": {"content": "Output Summary"}}]
        }
      },
      {
        "object": "block", 
        "type": "paragraph",
        "paragraph": {
          "rich_text": [{"text": {"content": "Brief description of what this output contains..."}}]
        }
      },
      {
        "object": "block",
        "type": "heading_2",
        "heading_2": {
          "rich_text": [{"text": {"content": "Generated Content"}}]
        }
      },
      {
        "object": "block",
        "type": "code",
        "code": {
          "language": "plain text",
          "rich_text": [{"text": {"content": "AI-generated content will be inserted here..."}}]
        }
      },
      {
        "object": "block",
        "type": "heading_2", 
        "heading_2": {
          "rich_text": [{"text": {"content": "Review Checklist"}}]
        }
      },
      {
        "object": "block",
        "type": "to_do",
        "to_do": {
          "rich_text": [{"text": {"content": "Accuracy review completed"}}],
          "checked": false
        }
      },
      {
        "object": "block",
        "type": "to_do",
        "to_do": {
          "rich_text": [{"text": {"content": "Aligns with project requirements"}}],
          "checked": false
        }
      },
      {
        "object": "block",
        "type": "to_do",
        "to_do": {
          "rich_text": [{"text": {"content": "Ready for implementation"}}],
          "checked": false
        }
      }
    ]
  }
}